
[bits 64]

global __inb
global __inw
global __inl

global __outb
global __outw
global __outl

__inb:
    in      di, al
    ret

__inw:
    in      di, ax
    ret

__inl:
    in      edi, eax
    ret

__outb:
    mov     ax, si
    out     di, al
    ret

__outw:
    out     di, si
    ret

__outl:
    out     edi, esi
    ret